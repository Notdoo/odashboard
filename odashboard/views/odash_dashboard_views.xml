<odoo>
    <record id="view_odash_dashboard_access_list" model="ir.ui.view">
        <field name="name">odash.dashboard.list</field>
        <field name="model">odash.dashboard</field>
        <field name="arch" type="xml">
            <list string="Accesses" create="0">
                <field name="user_id" />
                <field name="page_id" />
                <field name="last_authentication_date"/>
            </list>
        </field>
    </record>

     <record id="action_odash_dashboard_accesses" model="ir.actions.act_window">
        <field name="name">Pages</field>
        <field name="res_model">odash.dashboard</field>
        <field name="view_mode">list</field>
    </record>
</odoo>